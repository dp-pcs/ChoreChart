{
  "name": "chore-chart",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "mobile"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "build": "npm run build --workspace=web",
    "start": "npm run start --workspace=web",
    "lint": "npm run lint --workspace=web",
    "mobile:start": "npm run start --workspace=mobile",
    "mobile:android": "npm run android --workspace=mobile",
    "mobile:ios": "npm run ios --workspace=mobile",
    "mobile:web": "npm run web --workspace=mobile",
    "web:dev": "npm run dev --workspace=web",
    "web:build": "npm run build --workspace=web",
    "web:start": "npm run start --workspace=web",
    "install:all": "npm install && npm install --workspace=web && npm install --workspace=mobile",
    "db:migrate": "npm run db:migrate --workspace=web",
    "db:generate": "npm run db:generate --workspace=web",
    "db:studio": "npm run db:studio --workspace=web",
    "deploy:setup": "npm run deploy:setup --workspace=web"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.8"
  },
  "overrides": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  }
}
